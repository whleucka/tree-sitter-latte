{
  "name": "latte",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "layout_tag"
          },
          {
            "type": "SYMBOL",
            "name": "block_tag"
          }
        ]
      }
    },
    "layout_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{layout"
        },
        {
          "type": "SYMBOL",
          "name": "layout_path"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "block_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{block"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{/block}"
        }
      ]
    },
    "layout_path": {
      "type": "PATTERN",
      "value": "[^\\}]+"
    },
    "_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_\\x7f-\\xff][a-zA-Z0-9_\\x7f-\\xff]*"
    },
    "content": {
      "type": "PATTERN",
      "value": "[^{]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

