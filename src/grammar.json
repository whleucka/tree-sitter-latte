{
  "name": "latte",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "include_tag"
          },
          {
            "type": "SYMBOL",
            "name": "layout_tag"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "block_tag"
          },
          {
            "type": "SYMBOL",
            "name": "html_tag"
          },
          {
            "type": "SYMBOL",
            "name": "expression_tag"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "layout_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{layout"
        },
        {
          "type": "SYMBOL",
          "name": "file_path"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "file_path": {
      "type": "PATTERN",
      "value": "[^\\}]+"
    },
    "block_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{block"
        },
        {
          "type": "SYMBOL",
          "name": "block_name"
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "block_content"
          }
        },
        {
          "type": "STRING",
          "value": "{/block}"
        }
      ]
    },
    "block_name": {
      "type": "PATTERN",
      "value": "[^\\}]+"
    },
    "block_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "layout_tag"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "html_tag"
        },
        {
          "type": "SYMBOL",
          "name": "expression_tag"
        }
      ]
    },
    "include_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{include"
        },
        {
          "type": "SYMBOL",
          "name": "include_file_path"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "include_options"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "include_file_path": {
      "type": "PATTERN",
      "value": "[^,]+"
    },
    "include_options": {
      "type": "PATTERN",
      "value": "[^}]+"
    },
    "expression_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "expression_name"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "expression_name": {
      "type": "PATTERN",
      "value": "[^\\}]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{*"
        },
        {
          "type": "PATTERN",
          "value": "[^#]*\\#+([^\\}#][^#]*\\#+)*"
        },
        {
          "type": "STRING",
          "value": "*}"
        }
      ]
    },
    "html_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "PATTERN",
          "value": "[^\\{\\}>]+"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "text"
              },
              {
                "type": "SYMBOL",
                "name": "expression_tag"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "</"
        },
        {
          "type": "PATTERN",
          "value": "[^\\{\\}>]+"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[^{}<]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

